<!--pages/home/childCpns/w-recommend/w-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{recommend}}" wx:key="{{index}}">
    <view class="recommend-item ">
      <image src="{{item.image}}" mode="widthFix" bind:load="handleImageLoad"/>
      <view>
        <text>{{item.title}}</text>
      </view>
    </view>
  </block>
</view>
